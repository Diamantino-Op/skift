{
    "id": "stdcpp-shim",
    "type": "lib",
    "description": "The ISO C++ Standard C++ Library shim",
    "props": {
        "cpp-root-include": true
    },
    "enableIf": {
        "sys": [
            "efi",
            "hjert",
            "skift",
            "brutal"
        ]
    },
    "provides": [
        "stdcpp-impl"
    ]
}

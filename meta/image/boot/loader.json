{
    "title": "Welcome to skiftOS",
    "subtitle": "Select your form factor",
    "entries": [
        {
            "icon": "cellphone",
            "name": "Mobile",
            "kernel": "bundle://hjert/_bin",
            "blobs": [
                "bundle://system-srv/_bin",
                {
                    "url": "bundle://shell-app/_bin",
                    "props": {
                        "form-factor": "mobile"
                    }
                },
                "bundle://skift-wallpapers/images/abstract.qoi",
                "bundle://inter-font/fonts/Inter-Regular.ttf",
                "bundle://inter-font/fonts/Inter-Medium.ttf",
                "bundle://mdi-font/Material-Design-Icons.ttf"
            ]
        },
        {
            "icon": "laptop",
            "name": "Desktop",
            "kernel": "bundle://hjert/_bin",
            "blobs": [
                "bundle://system-srv/_bin",
                {
                    "url": "bundle://shell-app/_bin",
                    "props": {
                        "form-factor": "desktop"
                    }
                }
            ]
        },
        {
            "icon": "console",
            "name": "Headless",
            "kernel": "bundle://hjert/_bin",
            "blobs": [
                "bundle://system-srv/_bin"
            ]
        }
    ]
}
